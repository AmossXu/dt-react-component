{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/preset-react"
    ],
    "env": {
        "test": {
          "presets": ["@babel/preset-env", "@babel/preset-react"]
        }
    },
    "plugins": [
        // ["import", { "libraryName": "antd", "style": "true" }], // `style: true` 会加载 less 文件
        "@babel/transform-runtime",
        "macros",
        "@babel/plugin-proposal-class-properties"
        // "transform-require-context"
    ]
}